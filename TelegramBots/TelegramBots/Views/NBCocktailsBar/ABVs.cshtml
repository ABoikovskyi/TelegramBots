@model List<DataLayer.Models.NBCocktailsBar.ABV>

<h2>Крепость алкоголя</h2>
<form action="@Url.Action("ABV", "NBCocktailsBar")" class="create-button">
    <input type="submit" value="Добавить" />
</form>

<ul>
    @foreach (var fortress in Model)
    {
        <li><a href="@Url.Action("ABV", "NBCocktailsBar", new {id = fortress.Id})">@fortress.Name</a></li>
    }
</ul>